<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
  <item>
    <shape android:shape="oval">
      <size android:height="25dp" android:width="25dp"/>
      <solid android:color="#f7f7f7"/>
      <stroke android:color="#4A3D5A" android:width="1px"/>
    </shape>
  </item>

  <item android:bottom="1dp" android:top="1dp" android:left="1dp" android:right="1dp">
    <shape android:shape="oval">
      <size android:height="1dp" android:width="1dp"/>
      <solid android:color="#c7c7cc"/>
      <stroke android:color="#0f000000" android:width="2dp" />
    </shape>
  </item>
</layer-list>